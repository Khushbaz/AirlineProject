@model IEnumerable<AirlineProject.Models.Airline>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AirlineName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DepatureDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.AirlineName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DepatureDate)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.AirlineID">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.AirlineID">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.AirlineID">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
